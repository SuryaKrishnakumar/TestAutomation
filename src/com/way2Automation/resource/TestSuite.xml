<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd" >
<suite name="TestWay2Automation">
     <test name="testValidLogin">
        <parameter name="email" value="way2test2@gmail.com" />
        <parameter name="passwd" value="automation" />
         <parameter name="TCID" value="1" />
        <classes>
            <class name="com.way2Automation.test.TestLogin">
                <methods>
                    <include name="testLogin" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="testInvalidLogin1">
        <parameter name="email" value="way2" />
        <parameter name="passwd" value="automation" />
         <parameter name="TCID" value="2" />
        <classes>
            <class name="com.way2Automation.test.TestLogin">
                <methods>
                    <include name="testLogin" />
                </methods>
            </class>
        </classes>
    </test>
    <test name="testInvalidLogin2">
